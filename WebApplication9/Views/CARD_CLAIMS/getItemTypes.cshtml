@model IEnumerable<WebApplication9.Models.CARD_CLAIMS>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClaimType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimValue)
        </th>  
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClaimType)
        </td>
        <td>
          @Html.ActionLink(item.ClaimValue, "ItemsLayouts", new { id = item.Id })
        </td>
      
       
    </tr>
}

</table>
